<?js
var data = obj;
var self = this;
?>
<dl class="details">
<?js
var properties = data.properties;
if (properties && properties.length && properties.forEach) {
?>
  <h5 class="subsection-title">Properties:</h5>
  <dl><?js= this.partial('properties.tmpl', properties) ?></dl>
<?js
}
if (data.version) {
?>
  <dt class="tag-version icon-version" title="Version"></dt>
  <dd>
    <ul><li><?js= version ?></li></ul>
  </dd>
<?js
}
if (data.since) {
?>
  <dt class="tag-since icon-since" title="Since"></dt>
  <dd>
    <ul><li><?js= since ?></dd>
<?js
}
if (data.deprecated) {
?>
  <dt class="tag-deprecated icon-deprecated" title="Deprecated"></dt>
<?js
  if (data.deprecated === true) {
?>
  <dd class="yes-def">
    <ul><li>Yes</li></ul>
  </dd>
<?js
  } else {
?>
  <dd>
    <ul><li><?js= data.deprecated ?></li><ul>
  </dd>
<?js
  }
}
if (data.author && author.length) {
?>
  <dt class="tag-author icon-author" title="Author"></dt>
  <dd>
    <ul>
<?js
  author.forEach(function(a) {
?>
      <li><?js= self.resolveAuthorLinks(a) ?></li>
<?js
  });
?>
    </ul>
  </dd>
<?js
}
if (data.copyright) {
?>
  <dt class="tag-copyright icon-copyright" title="Copyright"></dt>
  <dd>
    <ul><li><?js= copyright ?></li></ul>
  </dd>
<?js
}
if (data.license) {
?>
  <dt class="tag-license icon-license" title="License"></dt>
  <dd>
    <ul><li><?js= license ?></li></ul>
  </dd>
<?js
}
if (data.defaultvalue) {
?>
  <dt class="tag-default icon-default" title="Default"></dt>
  <dd>
    <ul><li><?js= data.defaultvalue ?></li></ul>
  </dd>
<?js
}
if (data.tutorials && tutorials.length) {
?>
  <dt class="tag-tutorial icon-tutorial" title="Tutorial"></dt>
  <dd>
    <ul>
<?js
  tutorials.forEach(function(t) {
?>
      <li><?js= self.tutoriallink(t) ?></li>
<?js
  });
?>
    </ul>
  </dd>
<?js
}
if (data.see && see.length) {
?>
  <dt class="tag-see icon-see" title="See"></dt>
  <dd>
    <ul>
<?js
  see.forEach(function(s) {
?>
      <li><?js= self.linkto(s) ?></li>
<?js
  });
?>
    </ul>
  </dd>
<?js
}
if (data.todo && todo.length) {
?>
  <dt class="tag-todo icon-todo" title="TODO"></dt>
  <dd>
    <ul>
<?js
  todo.forEach(function(t) {
?>
      <li><?js= t ?></li>
<?js
  });
?>
    </ul>
  </dd>
<?js
}
?>
</dl>
